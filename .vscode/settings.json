{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.tabSize": 2,
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "always",
    "source.organizeImports": "always"
  },
  "cSpell.words": [
    "cssnano",
    "daisyui",
    "datepicker",
    "daygrid",
    "DDTHH",
    "fullcalendar",
    "httponly",
    "Parens",
    "preact",
    "Pretendard",
    "signout",
    "tailwindcss",
    "timegrid",
    "toastify"
  ]
}
